{
  "name": "@slashnephy/types-amq",
  "version": "0.4.8",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/types-amq"
  },
  "files": [
    "./package.json",
    "./types/index.d.ts",
    "./types/index.d.ts.map",
    "./README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "bump-version": "ts-node bin/bump-version.ts",
    "format": "concurrently -m 1 -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "tsc --project tsconfig.generate.json",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn npm publish"
  },
  "dependencies": {
    "@types/bootstrap": "3.3.33",
    "@types/cookieconsent": "3.1.1",
    "@types/jquery": "3.2.0",
    "@types/js-cookie": "2.2.7",
    "perfect-scrollbar": "0.7.0",
    "sweetalert": "2.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.0.17",
    "@slashnephy/prettier-config": "1.0.12",
    "@types/node": "20.4.10",
    "concurrently": "8.2.1",
    "eslint": "8.50.0",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.3",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "types": "./types/index.d.ts"
}